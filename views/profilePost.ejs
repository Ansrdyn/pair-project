  <table class="table">
    <tr>
      <th scope="col">name</th>
      <th scope="col">Photo Profile</th>
      <th scope="col">birthdate</th>
      <th scope="col">bio</th>
      <th scope="col">Total Post</th>
    </tr>
    <tr>
      <td scope="row">
        <%=data.name%>
      </td>
      <td>
        <%=data.photoProfile%>
      </td>
      <td>
        <%=data.getDate()%>
      </td>
      <td>
        <%=data.bio%>
      </td>
      <td>
        <%=data.dataValues.total%>
      </td>
      </tr>
     </table>
